{
  "version": "1.0.0",
  "packageManager": "pnpm@7.1.0",
  "private": true,
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "ts:lint": "eslint --ext .ts packages/*/{src,__tests__}/**.ts",
    "ts:format": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "js:format": "prettier --write \"packages/**/*.vue?(x)\"",
    "devtools": "vue-devtools"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vue/devtools": "^6.2.1",
    "eslint": "8.30.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "yorkie": "^2.0.0"
  }
}
